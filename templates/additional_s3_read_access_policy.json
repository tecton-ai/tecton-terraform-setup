{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": ${ADDITIONAL_S3_BUCKET_ARNS}
    },
    {
      "Effect": "Allow", 
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": ${ADDITIONAL_S3_OBJECT_ARNS}
    }
  ]
} 
{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListObject",
        "s3:HeadObject",
        "s3:PutObject"
      ],
      "Resource": [
        "${S3_LOG_DESTINATION}",
        "${S3_LOG_DESTINATION}/*"
      ]
    }
  ]
} 